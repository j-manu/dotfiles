alias ls='ls -G'
alias vi='vim'
alias mysql='/usr/local/mysql/bin/mysql'
export EDITOR=vim
alias mplayer='/Applications/MPlayer\ OSX\ Extended.app/Contents/Resources/External_Binaries/mplayer.app/Contents/MacOS/mplayer'
export PATH=$PATH:$HOME/bin:/usr/texbin:/usr/local/git/bin:/usr/local/mysql/bin:/usr/local/bin
source $HOME/bin/git-completion.sh
export PS1='[\W$(__git_ps1 " (%s)")]\$ '
export MAGICK_HOME="/usr/local"

